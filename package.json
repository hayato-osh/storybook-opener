{
  "name": "open-storybook-story",
  "displayName": "Open Storybook Story",
  "description": "Open Storybook Story",
  "version": "0.0.1",
  "publisher": "ygkn",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "open-storybook-story.open",
        "title": "Open Story",
        "category": "Open Storybook Story"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
