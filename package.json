{
  "name": "storybook-opener",
  "displayName": "Storybook Opener",
  "description": "A Visual Studio Code extension for effortless story opening",
  "version": "0.0.1",
  "publisher": "ygkn",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "homepage": "https://github.com/ygkn/storybook-opener#readme",
  "bugs": {
    "url": "https://github.com/ygkn/storybook-opener/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ygkn/storybook-opener.git"
  },
  "activationEvents": [
    "workspaceContains:.storybook"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "storybook-opener.open",
        "title": "Open Story",
        "category": "Storybook Opener"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.1",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.79.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "storybook": "^7.0.20",
    "typescript": "^5.1.3"
  }
}
